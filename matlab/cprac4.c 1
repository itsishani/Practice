#include<iostream>
#include<string>
using namespace std;
class event {
    
    public:
        string name;
        int duration;
        event(string s,int d){
            name= s;
            duration = d;
        }
        void displayEventDetails(string name,int duration){
            cout<<"The event "<<name<<" is occuring for "<<duration<<endl;
        }
};
class workshop: public event{
    public:
    string presenter;
    workshop(string p,string s,int d): event(s,d){
        presenter=p;
    }
    void displayWorkshopdetails(string presenter){
        cout<<"This workshop is conducted by "<<presenter<<endl;
    }
};
class conference : public event{
    public:
    string organizer;
    conference(string p,string s,int d): event(s,d){
        organizer = p;
    }
    void displayConferenceDetails(string organizer){
        cout<<"This conference is presented by "<<organizer<<endl;
    }
};
class TechConference: public conference{
    public:
    string domain;
    TechConference(string d,string p, string a, int w): conference(p,a,w){
        domain = d;
    }
    void TechConferenceDetails(string domain){
        cout<<"The domain of this tech conference is "<<domain<<endl;
    }
};

int main(){
    workshop ob("Atom","Strong",5);
    ob.displayEventDetails(ob.name,ob.duration);
    ob.displayWorkshopdetails(ob.presenter);
    TechConference obb("Programming","Team Atom","Robo",6);
    obb.displayEventDetails(obb.name,obb.duration);
    obb.displayConferenceDetails(obb.organizer);
    obb.TechConferenceDetails(obb.domain);
    return 0;
    
}


//power
#include<iostream>
using namespace std;
int power(int a,int b){
    int s = 1;
    for(int i=0;i<b;i++){
        s *= a;
    }
    return s;
}
int main()
{
    cout<<"Give the base and the power: ";
    int a, b;
    cin>>a>>b;
    cout<<power(a,b);
}




